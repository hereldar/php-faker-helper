{
    "name": "hereldar/faker-helper",
    "type": "library",
    "description": "Helper package to make it easier to generate fake values.",
    "keywords": ["faker"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/hereldar/php-faker-helper/issues",
        "source": "https://github.com/hereldar/php-faker-helper"
    },
    "authors": [
        {
            "name": "Samuel Maudo",
            "email": "samuelmaudo@gmail.com",
            "homepage": "https://github.com/samuelmaudo"
        }
    ],
    "require": {
        "fakerphp/faker": "^1.23",
        "webmozart/assert": "^1.0"
    },
    "require-dev": {
        "hereldar/coding-style": "^0.1.2",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^9.6",
        "psalm/plugin-phpunit": "^0.19.0",
        "vimeo/psalm": "^5.23.0"
    },
    "autoload": {
        "psr-4": {
            "Hereldar\\FakerHelper\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Hereldar\\FakerHelper\\Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
